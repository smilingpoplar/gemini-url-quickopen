  "type": "module",
  "devDependencies": {
    "babel-loader": "^10.0.0",
    "canvas": "^3.2.1",
    "extension": "^3.6.0"
  },
  "dependencies": {
    "@babel/core": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "js-yaml": "^4.1.1",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "webext-patterns": "^1.5.1"
  }
